/* 全局样式 */
.tab-bar-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  flex: 1;
}
/* 设置全局背景颜色为蔚蓝色 */


.tab-bar-text {
  text-align: center;
  font-size: 500px;
  color: rgb(20, 7, 136); /* 默认文字颜色 */
}

.tab-bar-text.active {
  color: #263766; /* 激活状态的文字颜色 */
}